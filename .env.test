# ============================================
# Palet8 Agents API - Test Environment
# ============================================
# Environment variables for testing

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8001
ENVIRONMENT=test
LOG_LEVEL=ERROR

# Google Gemini API (Test Keys)
GEMINI_API_KEY=test_gemini_api_key_12345
GEMINI_MODEL_BASE=gemini-2.5-flash-lite
GEMINI_MODEL_REASONING=gemini-2.5-flash
GEMINI_MODEL_IMAGE=gemini-2.5-flash-image
GEMINI_THINKING_BUDGET=default

# OpenAI API (Test Keys)
OPENAI_API_KEY=test_openai_api_key_67890
OPENAI_MODEL=gpt-4o
OPENAI_ORG_ID=

# Flux 1 Kontext API (Test Keys)
FLUX_API_KEY=test_flux_api_key_abcdef
FLUX_API_ENDPOINT=https://api.bfl.ai/v1
FLUX_MODEL_PRO=flux-1-kontext-pro
FLUX_MODEL_MAX=flux-1-kontext-max
FLUX_DEFAULT_MODEL=flux-1-kontext-pro

# Model Selection Defaults
DEFAULT_REASONING_MODEL=gemini
DEFAULT_IMAGE_MODEL=flux

# Cost Configuration (USD per request)
GEMINI_BASE_COST=0.00005
GEMINI_REASONING_COST=0.0001
GEMINI_IMAGE_COST=0.04
CHATGPT_REASONING_COST=0.005
FLUX_PRO_COST=0.04
FLUX_MAX_COST=0.055

# Performance Thresholds
MAX_REQUEST_TIMEOUT=300
GPU_ENABLED=false
ENABLE_STREAMING=true

# A2A Microservices (Disabled for tests)
USE_PRODUCT_A2A=false
PRODUCT_GENERATOR_URL=http://localhost:8081
A2A_AUTH_TOKEN=test_auth_token
A2A_ENABLE_TLS=false

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9091
ENABLE_STRUCTURED_LOGGING=false

# Language Constraints
ALLOWED_LANGUAGES='["en"]'
DEFAULT_LANGUAGE=en

# Google Cloud Project (Test Project)
GCP_PROJECT_ID=test-project-id
GOOGLE_CLOUD_PROJECT=test-project-id

# Python Configuration
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
